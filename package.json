{
  "name": "daa",
  "version": "1.1.0",
  "description": "daa",
  "author": "Igor Stepushchik <istepushchik@elilink.com> (https://elilink.com)",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": "^8.0.x"
  },
  "scripts": {
    "compile": "cross-env NODE_ENV=develop cross-env TASK=compile node ./tools/index",
    "migrate": "cross-env NODE_ENV=develop cross-env TASK=migrate node ./tools/index",
    "test": "cross-env NODE_ENV=develop cross-env TASK=test node ./tools/index",
    "test-compat": "truffle test",
    "coverage": "cross-env NODE_ENV=develop cross-env TASK=coverage node ./tools/index",
    "testrpc": "cross-env NODE_ENV=develop cross-env TASK=testrpc node ./tools/index",
    "testrpc-compat": "testrpc --gasPrice 1 --gasLimit 4712388 --port 8545 --mnemonic 'vital hedgehog drink result veteran juice jacket chat prepare small afraid orange'",
    "abi": "babel-node ./tools/abiEncode.js",
    "bundle": "shx rm -fr build; shx mkdir build; shx mkdir build/bundle; solcpiler --config-file .solcpiler.json",
    "migrate-rinkeby": "cross-env NODE_ENV=rinkeby babel-node ./tools/deploy.js",
    "migrate-mainnet": "yarn bundle; cross-env NODE_ENV=mainnet babel-node ./tools/deploy.js",
    "clean": "shx rm -fr dist build bin db coverage coverage.json coverage-js",
    "reset": "yarn clean; shx rm -fr node_modules",
    "upgrade": "yarn reset; yarn upgrade",
    "postinstall": "node ./tools/hashChecker.js",
    "docs:init": "doxity init",
    "docs:compile": "doxity compile",
    "docs:develop": "doxity develop",
    "docs:publish": "doxity publish",
    "docs:build": "doxity build"
  },
  "dependencies": {
    "chalk": "^2.3.0",
    "moment": "^2.19.3",
    "browser-builds": "git://github.com/ethereumjs/browser-builds.git",
    "ethereumjs-tx": "^1.3.3",
    "truffle-contract": "^3.0.1",
    "web3": "^1.0.0-beta.26",
    "zeppelin-solidity": "^1.6.0"
  },
  "devDependencies": {
    "@digix/doxity": "^0.5.2",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "browserify": "^14.5.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-bignumber": "^2.0.2",
    "cross-env": "^5.1.1",
    "eslint": "^4.12.1",
    "eslint-config-xo": "^0.19.0",
    "eslint-plugin-babel": "^4.1.2",
    "ethereumjs-abi": "^0.6.5",
    "ganache-core": "^2.0.2",
    "ethereumjs-testrpc": "^4.1.3",
    "jsdoc": "^3.5.5",
    "keythereum": "^1.0.2",
    "prompt": "^1.0.0",
    "shelljs": "^0.7.8",
    "shx": "^0.2.2",
    "solc": "0.4.18",
    "solcpiler": "^0.0.11",
    "solidity-coverage": "^0.4.9",
    "tree-kill": "^1.2.0",
    "truffle": "^4.0.1"
  },
  "optionalDevDependencies": {
    "fsevents": "*"
  }
}
